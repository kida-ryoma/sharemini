<!DOCTYPE html>
<html>
  <head>
    <title>Sharemini</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <link rel="stylesheet" href="style1.css">
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% if current_user %>
      <div class = "header">
        <%= render partial: "/posts/menu_bar" %>%
      </div>
    <% end %>
    <%= yield %>
  </body>
</html>
